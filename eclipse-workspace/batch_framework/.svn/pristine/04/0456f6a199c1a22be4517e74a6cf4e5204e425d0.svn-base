		<field  data-type="StringType"  name="no"    />
		<field  data-type="StringType"  name="user"     />
	</field-definition>
	
	<update-sql>
		  INSERT INTO #{schema}.TB_TEST
					(
					CBS_AC,
					USER_ID,
