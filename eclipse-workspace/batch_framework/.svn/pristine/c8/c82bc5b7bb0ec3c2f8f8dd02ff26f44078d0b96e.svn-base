package com.forms.framework.exception;

public class BatchDBException extends Exception
{

	private static final long serialVersionUID = 1L;

	public BatchDBException()
	{
		super();
	}

	public BatchDBException(String ip_message)
	{
		super(ip_message);
	}

	public BatchDBException(Throwable ip_cause)
	{
		super(ip_cause);
	}

	public BatchDBException(String ip_message, Throwable ip_cause)
	{
		super(ip_message, ip_cause);
	}

}
