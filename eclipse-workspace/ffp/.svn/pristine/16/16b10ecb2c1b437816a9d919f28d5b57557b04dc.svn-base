package com.forms.beneform4j.webapp.systemmanage.task.taskrule.bean;

import java.util.List;

import com.forms.beneform4j.webapp.common.bean.LogableBean;
import com.forms.beneform4j.webapp.systemmanage.role.bean.RoleBean;
import com.forms.beneform4j.webapp.systemmanage.user.bean.UserBean;

/**
 * Copy Right Information : Forms Syntron <br>
 * Project : 四方精创 Java EE 开发平台 <br>
 * Description : 任务规则bean<br>
 * Author : lsc <br>
 * Version : 1.0.0 <br>
 * Since : 1.0.0 <br>
 * Date : 2016-8-23<br>
 */
public class TaskRuleBean extends LogableBean {

    private static final long serialVersionUID = 3173576886527614393L;

    /**
     * 主键ID
     */
    private String ruleId;

    /**
     * 节点
     */
    private String taskId;

    /**
     * 节点名称
     */
    private String taskName;

    /**
     * 是否指定到人 Y：指定到人；N：不用到人
     */
    private String operFlag;

    /**
     * 限定人(list)
     */
    private List<UserBean> usersList;

    /**
     * 是否限定角色 Y：限定角色；N：不限定
     */
    private String roleFlag;

    /**
     * 限定角色值(list)
     */
    private List<RoleBean> rolesList;

    /**
     * 是否限定机构 Y：限定机构；N：不限定
     */
    private String orgFlag;

    /**
     * 限定机构值(list)
     */
    private List<TaskOrgBean> orgsList;

    /**
     * 是否进操作页面 Y：能进入操作页；N：只出列表页面
     */
    private String detailFlag;

    /**
     * 进入明细的menuid
     */
    private String detailMenuId;

    private String detailMenuName;

    /**
     * 菜单名称，用于查询菜单
     */
    private String menuName;

    /**
     * 菜单ID
     */
    private String menuId;

    /**
     * 当前处理人
     */
    private String operUser;

    private String Text;

    public String getTaskId() {
        return taskId;
    }

    public void setTaskId(String taskId) {
        this.taskId = taskId;
    }

    public String getTaskName() {
        return taskName;
    }

    public void setTaskName(String taskName) {
        this.taskName = taskName;
    }

    public String getDetailFlag() {
        return detailFlag;
    }

    public void setDetailFlag(String detailFlag) {
        this.detailFlag = detailFlag;
    }

    public String getDetailMenuId() {
        return detailMenuId;
    }

    public void setDetailMenuId(String detailMenuId) {
        this.detailMenuId = detailMenuId;
    }

    public String getDetailMenuName() {
        return detailMenuName;
    }

    public void setDetailMenuName(String detailMenuName) {
        this.detailMenuName = detailMenuName;
    }

    public String getRuleId() {
        return ruleId;
    }

    public void setRuleId(String ruleId) {
        this.ruleId = ruleId;
    }

    public String getOperFlag() {
        return operFlag;
    }

    public void setOperFlag(String operFlag) {
        this.operFlag = operFlag;
    }

    public String getRoleFlag() {
        return roleFlag;
    }

    public void setRoleFlag(String roleFlag) {
        this.roleFlag = roleFlag;
    }

    public String getOrgFlag() {
        return orgFlag;
    }

    public void setOrgFlag(String orgFlag) {
        this.orgFlag = orgFlag;
    }

    public List<UserBean> getUsersList() {
        return usersList;
    }

    public void setUsersList(List<UserBean> usersList) {
        this.usersList = usersList;
    }

    public List<RoleBean> getRolesList() {
        return rolesList;
    }

    public void setRolesList(List<RoleBean> rolesList) {
        this.rolesList = rolesList;
    }

    public List<TaskOrgBean> getOrgsList() {
        return orgsList;
    }

    public void setOrgsList(List<TaskOrgBean> orgsList) {
        this.orgsList = orgsList;
    }

    public String getMenuName() {
        return menuName;
    }

    public void setMenuName(String menuName) {
        this.menuName = menuName;
    }

    public String getMenuId() {
        return menuId;
    }

    public void setMenuId(String menuId) {
        this.menuId = menuId;
    }

    public String getOperUser() {
        return operUser;
    }

    public void setOperUser(String operUser) {
        this.operUser = operUser;
    }

    public String getText() {
        return Text;
    }

    public void setText(String text) {
        Text = text;
    }

}
