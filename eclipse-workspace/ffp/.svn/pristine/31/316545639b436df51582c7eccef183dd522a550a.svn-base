package com.forms.ffp.webapp.cashmanagement.return_refund.bean;

import java.util.Date;

import com.forms.beneform4j.webapp.common.bean.LogableBean;

public class ReturnRefundBean extends LogableBean{
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	private String jnlNo;
	private String txCode;
	private String settlementAmt;
	private Date settlementDate;
	private String debtorName;
	private String debtorAcctno;
	private String creditorName;
	private String creditorAcctno;
	public String getJnlNo() {
		return jnlNo;
	}
	public String getTxCode() {
		return txCode;
	}
	public String getSettlementAmt() {
		return settlementAmt;
	}
	public Date getSettlementDate() {
		return settlementDate;
	}
	public String getDebtorName() {
		return debtorName;
	}
	public String getDebtorAcctno() {
		return debtorAcctno;
	}
	public String getCreditorName() {
		return creditorName;
	}
	public String getCreditorAcctno() {
		return creditorAcctno;
	}
	public void setJnlNo(String jnlNo) {
		this.jnlNo = jnlNo;
	}
	public void setTxCode(String txCode) {
		this.txCode = txCode;
	}
	public void setSettlementAmt(String settlementAmt) {
		this.settlementAmt = settlementAmt;
	}
	public void setSettlementDate(Date settlementDate) {
		this.settlementDate = settlementDate;
	}
	public void setDebtorName(String debtorName) {
		this.debtorName = debtorName;
	}
	public void setDebtorAcctno(String debtorAcctno) {
		this.debtorAcctno = debtorAcctno;
	}
	public void setCreditorName(String creditorName) {
		this.creditorName = creditorName;
	}
	public void setCreditorAcctno(String creditorAcctno) {
		this.creditorAcctno = creditorAcctno;
	}
}
