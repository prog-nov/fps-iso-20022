package com.forms.ffp.core.exception;

public class FFPDBException extends Exception
{

	private static final long serialVersionUID = 1L;

	public FFPDBException()
	{
		super();
	}

	public FFPDBException(String ip_message)
	{
		super(ip_message);
	}

	public FFPDBException(Throwable ip_cause)
	{
		super(ip_cause);
	}

	public FFPDBException(String ip_message, Throwable ip_cause)
	{
		super(ip_message, ip_cause);
	}

}
