<%@ page language="java" pageEncoding="UTF-8"%>
<%@ include file="/WEB-INF/view/common/taglib.jsp"%>
<form id="settingForm" class="easyui-form" method="post">
				<div class="bf4j-group">
					<div class="bf4j-group-content">
						<div class="bf4j-line">
							<div class="bf4j-cell bf4j-c2">
								<label><spring:message code="ffp.systemmaintenance.systemstat.curDate" /></label>
							</div>
							<div class="bf4j-cell bf4j-sc4">
								<span class="bf4j-input">
									<input type="text" name="batchAcdt" class="easyui-textbox" style="width: 90%" readonly disabled/>
								</span>
							</div>
						</div>
						<div class="bf4j-line">
							<div class="bf4j-cell bf4j-c2">
								<label><spring:message code="ffp.systemmaintenance.systemstat.runningMode" /></label>
							</div>
							<div class="bf4j-cell bf4j-sc4">
								<span class="bf4j-input">
									<input type="text" name="runningModeLabel" class="easyui-textbox" style="width: 90%" readonly disabled/>
									<input name="runningMode" id="runningMode" type="hidden">
								</span>
							</div>
						</div>
						<div class="bf4j-line">
							<div class="bf4j-cell bf4j-c2">
								<label><spring:message code="ffp.systemmaintenance.systemstat.fpsReceiveMode" /></label>
							</div>
							<div class="bf4j-cell bf4j-sc4">
								<span class="bf4j-input">
									<input type="text" name="fpsReceiveModeLabel" class="easyui-textbox" style="width: 90%" readonly disabled/>
									<input name="fpsReceiveMode" id="fpsReceiveMode" type="hidden">
								</span>
							</div>
							<div class="bf4j-cell bf4j-c2 bf4j-left">
								<a class="bf4j-btn bf4j-btn-save" id="switchReceiveModeButton"></a>
							</div>
						</div>
						<div class="bf4j-line">
							<div class="bf4j-cell bf4j-c2">
								<label><spring:message code="ffp.systemmaintenance.systemstat.realtimeListenerStat" /></label>
							</div>
							<div class="bf4j-cell bf4j-sc4">
								<span class="bf4j-input">
									<input type="text" name="realtimeListenerStatLabel" class="easyui-textbox" style="width: 90%" readonly disabled/>
									<input name="realtimeListenerStat" id="realtimeListenerStat" type="hidden">
								</span>
							</div>
							<div class="bf4j-cell bf4j-c2 bf4j-left">
								<a class="bf4j-btn bf4j-btn-save" id="switchRealtimeListenerStatButton"></a>
							</div>
						</div>
						<div class="bf4j-line">
							<div class="bf4j-cell bf4j-c2">
								<label><spring:message code="ffp.systemmaintenance.systemstat.realtimeControlStat" /></label>
							</div>
							<div class="bf4j-cell bf4j-sc4">
								<span class="bf4j-input">
									<input type="text" name="realtimeControlStatLabel" class="easyui-textbox" style="width: 90%" readonly disabled/>
									<input name="realtimeControlStat" id="realtimeControlStat" type="hidden">
								</span>
							</div>
							<div class="bf4j-cell bf4j-c2 bf4j-left">
								<a class="bf4j-btn bf4j-btn-save" id="switchRealtimeControlButton"></a>
							</div>
						</div>
						<div class="bf4j-line">
							<div class="bf4j-cell bf4j-c2">
								<label><spring:message code="ffp.systemmaintenance.systemstat.batchListenerStat" /></label>
							</div>
							<div class="bf4j-cell bf4j-sc4">
								<span class="bf4j-input">
									<input type="text" name="batchListenerStatLabel" class="easyui-textbox" style="width: 90%" readonly disabled/>
									<input name="batchListenerStat" id="batchListenerStat" type="hidden">
								</span>
							</div>
						</div>
					</div>
				</div>
			</form>