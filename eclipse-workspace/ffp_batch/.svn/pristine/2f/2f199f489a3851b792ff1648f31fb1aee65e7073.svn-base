SET SCHEMA RTCXU01;
DROP TABLE RTCXU01.B_INQUIRY_USER;

CREATE TABLE RTCXU01.B_INQUIRY_USER
	(
	USER_ID       VARCHAR (10) NOT NULL,
	ACTIVE        CHARACTER (1),
	USER_TYPE     CHARACTER (1),
	USER_GROUP    VARCHAR (200),
	NET_NO        VARCHAR (7),
	RULE_NO       VARCHAR (5),
	NOTES_ADDRESS VARCHAR (50),
	START_DATE    VARCHAR (10),
	END_DATE      VARCHAR (10)
	)
	IN BASESPACE
	INDEX IN MSGINDEXSPACE;

ALTER TABLE RTCXU01.B_INQUIRY_USER DATA CAPTURE NONE;
ALTER TABLE RTCXU01.B_INQUIRY_USER NOT VOLATILE;
ALTER TABLE RTCXU01.B_INQUIRY_USER APPEND OFF;
ALTER TABLE RTCXU01.B_INQUIRY_USER LOCKSIZE ROW;

COMMENT ON COLUMN RTCXU01.B_INQUIRY_USER.USER_ID IS '用户id';
COMMENT ON COLUMN RTCXU01.B_INQUIRY_USER.ACTIVE IS '活动状态';
COMMENT ON COLUMN RTCXU01.B_INQUIRY_USER.USER_TYPE IS '柜员类型';
COMMENT ON COLUMN RTCXU01.B_INQUIRY_USER.USER_GROUP IS '用户组';
COMMENT ON COLUMN RTCXU01.B_INQUIRY_USER.NET_NO IS '网点';
COMMENT ON COLUMN RTCXU01.B_INQUIRY_USER.RULE_NO IS '管辖网点';
COMMENT ON COLUMN RTCXU01.B_INQUIRY_USER.NOTES_ADDRESS IS 'notes地址';
COMMENT ON COLUMN RTCXU01.B_INQUIRY_USER.START_DATE IS '柜员生效日';
COMMENT ON COLUMN RTCXU01.B_INQUIRY_USER.END_DATE IS '柜员结束日';

CREATE UNIQUE INDEX RTCXU01.PK_B_INQUIRY_USER
	ON RTCXU01.B_INQUIRY_USER (USER_ID) ALLOW REVERSE SCANS;

