SET SCHEMA RTCXU01;
DROP TABLE RTCXU01.T_OVERSEAS_ARCHIVES;

CREATE TABLE RTCXU01.T_OVERSEAS_ARCHIVES
	(
	SYS_ARCHIVES_NO VARCHAR (18) NOT NULL,
	ARCHIVES_NO     VARCHAR (18) NOT NULL,
	TABLE_NAME      VARCHAR (128) NOT NULL,
	RTL_NO          VARCHAR (18) NOT NULL,
	SEND_MSG_NO     VARCHAR (18),
	RLT_OREF_NO     VARCHAR (18),
	SUMMARY_NO      VARCHAR (5),
	SUMMARY_LEVEL   VARCHAR (1),
	SUMMARY_TYPE    VARCHAR (1),
	TRANS_DATE      VARCHAR (10),
	TRANS_TIME      VARCHAR (8),
	MSG_TYPE        VARCHAR (5),
	CURR_CODE       VARCHAR (3),
	TRADE_AMT       DECIMAL (18,2),
	BANK_NO         VARCHAR (11),
	TRANS_NET       VARCHAR (7),
	CURR_NET        VARCHAR (7),
	OPER_ID         VARCHAR (20),
	MSG_STATE       VARCHAR (5),
	MERGER_DATE     DATE NOT NULL,
	MERGER_TIME     TIME NOT NULL,
	MERGER_USER     VARCHAR (20) NOT NULL,
	CASE_STATUS     VARCHAR (1) DEFAULT 'N',
	ENDCASE_DATE    VARCHAR (10),
	RECV_OREF_NO    VARCHAR (18),
	CONSTRAINT KEY PRIMARY KEY (RTL_NO, TABLE_NAME)
	)
	IN MSGSPACE
	INDEX IN MSGINDEXSPACE;

ALTER TABLE RTCXU01.T_OVERSEAS_ARCHIVES DATA CAPTURE NONE;
ALTER TABLE RTCXU01.T_OVERSEAS_ARCHIVES NOT VOLATILE;
ALTER TABLE RTCXU01.T_OVERSEAS_ARCHIVES APPEND OFF;
ALTER TABLE RTCXU01.T_OVERSEAS_ARCHIVES LOCKSIZE ROW;

COMMENT ON COLUMN RTCXU01.T_OVERSEAS_ARCHIVES.SYS_ARCHIVES_NO IS '内部档案号,系统内部管理档案用';
COMMENT ON COLUMN RTCXU01.T_OVERSEAS_ARCHIVES.ARCHIVES_NO IS '业务要求的档案头';
COMMENT ON COLUMN RTCXU01.T_OVERSEAS_ARCHIVES.TABLE_NAME IS '相关编号存在的表名';
COMMENT ON COLUMN RTCXU01.T_OVERSEAS_ARCHIVES.RTL_NO IS '其中包括来报编号、往报编号、支付编号、退汇编号';
COMMENT ON COLUMN RTCXU01.T_OVERSEAS_ARCHIVES.SEND_MSG_NO IS '20场编号，收付INCOMING、OUTGOING 、往报、来报的发报行编号';
COMMENT ON COLUMN RTCXU01.T_OVERSEAS_ARCHIVES.RLT_OREF_NO IS '21场编号，收付INCOMING、OUTGOING 、往报、来报的有关业务编号';
COMMENT ON COLUMN RTCXU01.T_OVERSEAS_ARCHIVES.SUMMARY_NO IS '摘要序号';
COMMENT ON COLUMN RTCXU01.T_OVERSEAS_ARCHIVES.SUMMARY_LEVEL IS '摘要网点级别';
COMMENT ON COLUMN RTCXU01.T_OVERSEAS_ARCHIVES.SUMMARY_TYPE IS '摘要类型';
COMMENT ON COLUMN RTCXU01.T_OVERSEAS_ARCHIVES.TRANS_DATE IS '交易日期';
COMMENT ON COLUMN RTCXU01.T_OVERSEAS_ARCHIVES.TRANS_TIME IS '如果是查询来报则此处填查询报文时间 如果是查询往报则此处填发报时间 如果是支付报文则此处填支付时间 如果是退汇报文则此处填退汇时间 如果是内部报文则此处填发报时间';
COMMENT ON COLUMN RTCXU01.T_OVERSEAS_ARCHIVES.MSG_TYPE IS '报文类型';
COMMENT ON COLUMN RTCXU01.T_OVERSEAS_ARCHIVES.CURR_CODE IS '币别';
COMMENT ON COLUMN RTCXU01.T_OVERSEAS_ARCHIVES.TRADE_AMT IS '金额';
COMMENT ON COLUMN RTCXU01.T_OVERSEAS_ARCHIVES.BANK_NO IS '行号';
COMMENT ON COLUMN RTCXU01.T_OVERSEAS_ARCHIVES.TRANS_NET IS '交易网点';
COMMENT ON COLUMN RTCXU01.T_OVERSEAS_ARCHIVES.CURR_NET IS '当前网点';
COMMENT ON COLUMN RTCXU01.T_OVERSEAS_ARCHIVES.OPER_ID IS '操作ID';
COMMENT ON COLUMN RTCXU01.T_OVERSEAS_ARCHIVES.MSG_STATE IS '来报：200来报未处理  来报中的处理标志（WITHIN_DEAL_FLAG）+ 处理状态（DEAL_STATUS）构成档案表中的状态 外部往报 1+往报的报文状态(MSG_STATE) 内部报文';
COMMENT ON COLUMN RTCXU01.T_OVERSEAS_ARCHIVES.MERGER_DATE IS '该交易归档日期';
COMMENT ON COLUMN RTCXU01.T_OVERSEAS_ARCHIVES.MERGER_TIME IS '该交易归档时间';
COMMENT ON COLUMN RTCXU01.T_OVERSEAS_ARCHIVES.MERGER_USER IS '该交易归档时的操作员';
COMMENT ON COLUMN RTCXU01.T_OVERSEAS_ARCHIVES.CASE_STATUS IS '档案状态';
COMMENT ON COLUMN RTCXU01.T_OVERSEAS_ARCHIVES.ENDCASE_DATE IS '结案日期';
COMMENT ON COLUMN RTCXU01.T_OVERSEAS_ARCHIVES.RECV_OREF_NO IS '来报的编号：自动勾连时使用';

