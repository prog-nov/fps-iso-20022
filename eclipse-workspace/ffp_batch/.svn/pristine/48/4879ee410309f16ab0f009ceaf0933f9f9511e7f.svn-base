<job-sql-configs>
	<transaction>
		<sql>
			DELETE FROM #{schema}.D_OVERSEAS_PAY_OUTGO_TEMP
		</sql>
	</transaction>
	<transaction>
		<sql>
			INSERT INTO #{schema}.D_OVERSEAS_PAY_OUTGO_TEMP
			(
					OREF_NO,		DATA_FLAG,
					MSG_TYPE,		MSG_STATUS,		ORG_RECV_BANK,		ORG_RECV_DATE,		ORG_SEND_BANK,
			 		ORG_SEND_DATE,	SEND_MSGNO,		RLT_OREF_NO,		BANK_OPER_CODE,		VAL_DATE,
					TRADE_CCY,		TRADE_AMT,		ORG_PAY_CCY,		ORG_PAY_AMT,		REMIT,
					REMIT_ACT,		REMIT_BKNO,		REMIT_BKNM,			AG_SEND_BKNO,		AG_SEND_BKNM,
			 		AG_RECV_BKNO,	AG_RECV_BKNM,	LIQUI_BIC,			LIQUIDATE_BKNM,		MID_BKNO,
					MID_BKNM,		ACT_BKNO,		ACT_BKNM,			GATHERING_BKNO,		GATHERING_BKNM,
					BENEFIT_ACT,	BENEFICIARY,	PAY_MEMO,			DTCHG,				RCVBANK_CCY,
					RCVBANK_AMT,	MEMO,			STL_CASE_NO,		STL_TEMP_ID,		STL_INV_PURP,
					BRANCH,			INQUIRY_NO,		OSN,				ISN,				RATE,
					RECV_ACT,		REMIT_BANK_ACTNO,SDEDC,				ACT_VAL_DATE,		PAY_NO
			)
			SELECT 	A.CP04_BUSINESS_NO OREF_NO,
					'N',
					A.CP04_MSG_TYPE,  		
					A.CP04_PROS_FLAG,	
					SUBSTR(B.RECV_BANK,1,8)||SUBSTR(B.RECV_BANK,10,3) AS ORG_RECV_BANK,
					B.OUTPUT_DATE,
					SUBSTR(B.S_R_ORG_BANK,1,8)||SUBSTR(B.S_R_ORG_BANK,10,3) AS ORG_SEND_BANK,
					SUBSTR(A.DP04_SEND_DATE,1,4)||'-'||SUBSTR(A.DP04_SEND_DATE,5,2)||'-'||SUBSTR(A.DP04_SEND_DATE,7,2) AS SEND_DATE,
					B.SIT_20,
					B.SIT_21,
					B.SIT_23B,
					SUBSTR(A.DP04_VALU_DATE,1,4)||'-'||SUBSTR(A.DP04_VALU_DATE,5,2)||'-'||SUBSTR(A.DP04_VALU_DATE,7,2) AS VALUE_DATE,
					A.CP04_PAY_CURR,
					DECIMAL(REPLACE(A.CP04_PAY_AMT, ',', '.'),18,2) AS TRADE_AMT,
					SUBSTR(B.SIT_33B, 1, 3) AS ORG_PAY_CCY,
					DECIMAL(REPLACE(SUBSTR(B.SIT_33B, 4, 15), ',', '.'), 18, 2) AS ORG_PAY_AMT,
					B.SIT_50_REMIT,
					B.SIT_50_REMIT_AC,
					B.SIT_52_REMIT_BANK_BIC,
					B.SIT_52_REMIT_BANK,
					B.SIT_53_AG_SEND_BANK_BIC,
					B.SIT_53_AG_SEND_BANK,
					B.SIT_54_AG_RECV_BANK_BIC,
					B.SIT_54_AG_RECV_BANK,
					B.SIT_55_LIQUI_BANK_BIC,
					B.SIT_55_LIQUI_BANK,
					B.SIT_56_MID_BKNM_BIC,
					B.SIT_56_MID_BKNM,
					B.SIT_57_ACT_BKNM_BIC,
					B.SIT_57_ACT_BKNM,
					B.SIT_58_GATHERING_BIC,
					B.SIT_58_GATHERING,
					B.SIT_59_BENEFIT_ACT,
					B.SIT_59_BENEFICIARY,
					B.SIT_70,
					B.SIT_71A,
					SUBSTR(B.SIT_71G,1,3) AS RCVBANK_CCY,
					DECIMAL(REPLACE(SUBSTR(B.SIT_71G,4,15), ',', '.'), 18, 2) AS RCVBANK_AMT,
					B.SIT_72,
					A.CP04_STL_CASE_NO,
					A.CP04_STL_TEMP_ID,
					A.CP04_STL_INV_PURP,
					A.SPYO_BRANCH,
					A.CP04_STL_CASE_NO,
					B.SESSION_NUMBER,
					B.IO_SEQ_NUMBER,
					REPLACE(B.SIT_36, ',', '.') AS RATE,
					CASE WHEN B.SIT_58_GATHERING_ACT LIKE '/_/%' THEN SUBSTR(B.SIT_58_GATHERING_ACT,2,1)||SUBSTR(B.SIT_58_GATHERING_ACT,4,34)
						WHEN B.SIT_58_GATHERING_ACT LIKE '/%' THEN SUBSTR(B.SIT_58_GATHERING_ACT,2,35)
						ELSE B.SIT_58_GATHERING_ACT
					END AS RECV_ACT,
					CASE WHEN B.SIT_52_REMIT_BANK_ACNO LIKE '/_/%' THEN SUBSTR(B.SIT_52_REMIT_BANK_ACNO,2,1)||SUBSTR(B.SIT_52_REMIT_BANK_ACNO,4,34)
						WHEN B.SIT_52_REMIT_BANK_ACNO LIKE '/%' THEN SUBSTR(B.SIT_52_REMIT_BANK_ACNO,2,35)
						ELSE B.SIT_52_REMIT_BANK_ACNO
					END AS REMIT_BANK_ACTNO,
					LEFT(B.SIT_77, 109) SDEDC,
					'20'||SUBSTR(B.SIT_30,1,2)||'-'||SUBSTR(B.SIT_30,3,2)||'-'||SUBSTR(B.SIT_30,5,2) ACT_VAL_DATE,
					CP04_PAY_NO
			FROM #{schema}.D_OVERSEAS_FILE_P31_TEMP A
			JOIN #{schema}.D_OVERSEAS_PAY_OUTGO_MSGDATA_TEMP B
			ON A.CP04_INST=B.INST_NO
			AND A.CP04_PAY_NO = B.PAY_NO
			AND A.CP04_MSG_SEQ_NO = B.REC_SEQ_NO
			AND A.CP04_APPG_DATE = B.APPG_DATE
			WHERE A.CP04_MSG_TYPE IN ('MT103', 'MT110', 'MT202')
				AND A.CP04_APPG_DATE = REPLACE('#{acDate}','-','')
		</sql>
	</transaction>
	<transaction>
		<sql>
			DELETE FROM #{schema}.D_OVERSEAS_PAY_SUB_TEMP A
		</sql>
	</transaction>
	<transaction>
		<sql>
			INSERT INTO #{schema}.D_OVERSEAS_PAY_SUB_TEMP
					(OREF_NO, 
					SITMODNO, 	SEQ_NO, 	IPMRVDAT, 
					SITISTCD, 	SITSCCUR, 	SITSCAMT, 	DATA_FLAG)
			SELECT 	B.CP04_BUSINESS_NO OREF_NO,
					'0',		A.SEQ_NO,	CHAR(CURRENT DATE),
					A.SITISTCD,	A.SITSCCUR,	A.SITSCAMT,	'N'
			FROM #{schema}.D_OVERSEAS_PAY_OUTGO_MSGDATA_SUB_TEMP A
			JOIN #{schema}.D_OVERSEAS_FILE_P31_TEMP B
			ON A.APPG_DATE = B.CP04_APPG_DATE
			AND A.INST_NO = B.CP04_INST
			AND A.PAY_NO = B.CP04_PAY_NO
			AND A.REC_SEQ_NO = B.CP04_MSG_SEQ_NO
			WHERE A.APPG_DATE = REPLACE('#{acDate}', '-', '')
		</sql>
	</transaction>
</job-sql-configs>