
--1.添加网点机构号映射表(上次在生产库上已添加此表，需要检查下 字段，长度，以及tablespace.目前用的"BASESPACE"应该不正确)
   
SET SCHEMA RTCXU01;
CREATE TABLE RTCXU01.B_NETNO_MAPPING
	(
	ORG_ID     VARCHAR (5),
	NET_NO     VARCHAR (5),
	RULE_NET   VARCHAR (5),
	NET_NAME   VARCHAR (100),
	ORG_TYPE   VARCHAR (1),
	NET_ADDR   VARCHAR (100),
	NET_LEVEL  VARCHAR (1),
	LEVEL_CODE VARCHAR (1),
	SNGFLG     VARCHAR (1),
	SECBNK     VARCHAR (7),
	NET_IP     VARCHAR (15),
	INS_DATE   DATE,
	INS_TIME   TIME,
	INS_MGR    VARCHAR (30),
	MODI_DATE  DATE,
	MODI_TIME  TIME,
	MODI_MGR   VARCHAR (30)
	)
	IN BASESPACE
	INDEX IN MSGINDEXSPACE

   ALTER TABLE RTCXU01.B_NETNO_MAPPING DATA CAPTURE NONE;
   ALTER TABLE RTCXU01.B_NETNO_MAPPING NOT VOLATILE;
   ALTER TABLE RTCXU01.B_NETNO_MAPPING APPEND OFF;
   ALTER TABLE RTCXU01.B_NETNO_MAPPING LOCKSIZE ROW;
   