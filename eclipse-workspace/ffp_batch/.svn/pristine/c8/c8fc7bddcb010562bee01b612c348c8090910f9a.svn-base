SET SCHEMA RTCXU01;
CREATE TABLE B_BH_JOB_JNL
	(
	BATCH_AC_DATE VARCHAR (10) NOT NULL,
	JOB_ID        VARCHAR (64) NOT NULL,
	JNL_SEQ       INTEGER NOT NULL,
	JOB_STAT      VARCHAR (1),
	RESULT        VARCHAR (1),
