SET SCHEMA RTCXU01;
DROP TABLE RTCXU01.B_OREF_GENERAL;

CREATE TABLE RTCXU01.B_OREF_GENERAL
	(
	ORG_NO        VARCHAR (5) NOT NULL,
	TRADE_DATE    DATE NOT NULL,
	OSM_SEND_OREF VARCHAR (18),
	OSM_RECV_OREF VARCHAR (18),
	WSM_SEND_OREF VARCHAR (18)
	)
	IN BASESPACE
	INDEX IN MSGINDEXSPACE;

ALTER TABLE RTCXU01.B_OREF_GENERAL DATA CAPTURE NONE;
ALTER TABLE RTCXU01.B_OREF_GENERAL NOT VOLATILE;
ALTER TABLE RTCXU01.B_OREF_GENERAL APPEND OFF;
ALTER TABLE RTCXU01.B_OREF_GENERAL LOCKSIZE ROW;
COMMENT ON TABLE RTCXU01.B_OREF_GENERAL IS '业务编号生成表';

COMMENT ON COLUMN RTCXU01.B_OREF_GENERAL.ORG_NO IS '机构号';
COMMENT ON COLUMN RTCXU01.B_OREF_GENERAL.TRADE_DATE IS '交易日期';
COMMENT ON COLUMN RTCXU01.B_OREF_GENERAL.OSM_SEND_OREF IS '国外发报业务编号 生成规则：10+机构号+YYMMDD+5位序号';
COMMENT ON COLUMN RTCXU01.B_OREF_GENERAL.OSM_RECV_OREF IS '国外收报业务编号 生成规则：20+101+YYMMDD+5位序号';
COMMENT ON COLUMN RTCXU01.B_OREF_GENERAL.WSM_SEND_OREF IS '国外发报业务编号 生成规则：30+机构号+YYMMDD+5位序号';

