<job-sql-configs>
	<transaction>
		<sql>
			<![CDATA[
				INSERT INTO #{schema}.M_OVERSEAS_RECV_#{agoYear}
				SELECT * FROM #{schema}.M_OVERSEAS_RECV A
		      	WHERE A.ORG_SEND_DATE <= '#{yearAgo}'
		  	]]>
	  	</sql>
	</transaction>
	<transaction>
		<sql>
			<![CDATA[
				DELETE FROM #{schema}.M_OVERSEAS_RECV A
		      	WHERE A.ORG_SEND_DATE <= '#{yearAgo}'
		  	]]>
	  	</sql>
	</transaction>
</job-sql-configs>