SET SCHEMA RTCXU01;
DROP TABLE RTCXU01.D_OVERSEAS_HISTORY;

CREATE TABLE RTCXU01.D_OVERSEAS_HISTORY
	(
	TABLE_NAME VARCHAR (128) NOT NULL,
	YEAR       VARCHAR (4) NOT NULL
	)
	IN BASESPACE
	INDEX IN MSGINDEXSPACE;

ALTER TABLE RTCXU01.D_OVERSEAS_HISTORY DATA CAPTURE NONE;
ALTER TABLE RTCXU01.D_OVERSEAS_HISTORY NOT VOLATILE;
ALTER TABLE RTCXU01.D_OVERSEAS_HISTORY APPEND OFF;
ALTER TABLE RTCXU01.D_OVERSEAS_HISTORY LOCKSIZE ROW;

COMMENT ON COLUMN RTCXU01.D_OVERSEAS_HISTORY.TABLE_NAME IS '收付档表名';
COMMENT ON COLUMN RTCXU01.D_OVERSEAS_HISTORY.YEAR IS '年份';

