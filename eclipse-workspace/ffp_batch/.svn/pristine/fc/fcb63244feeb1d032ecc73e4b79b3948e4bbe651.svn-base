SET SCHEMA RTCXU01;
DROP TABLE RTCXU01.B_MSG_STATUS_MAP;

CREATE TABLE RTCXU01.B_MSG_STATUS_MAP
	(
	TABLE_NAME      VARCHAR (20) NOT NULL,
	MSG_STATUS      VARCHAR (4) NOT NULL,
	MSG_STATUS_NAME VARCHAR (100),
	PRIMARY KEY (MSG_STATUS, TABLE_NAME)
	)
	IN BASESPACE;

ALTER TABLE RTCXU01.B_MSG_STATUS_MAP DATA CAPTURE NONE;
ALTER TABLE RTCXU01.B_MSG_STATUS_MAP NOT VOLATILE;
ALTER TABLE RTCXU01.B_MSG_STATUS_MAP APPEND OFF;
ALTER TABLE RTCXU01.B_MSG_STATUS_MAP LOCKSIZE ROW;

COMMENT ON COLUMN RTCXU01.B_MSG_STATUS_MAP.TABLE_NAME IS '表名';
COMMENT ON COLUMN RTCXU01.B_MSG_STATUS_MAP.MSG_STATUS IS '支付报文报文状态';
COMMENT ON COLUMN RTCXU01.B_MSG_STATUS_MAP.MSG_STATUS_NAME IS '报文状态对应的中文描述';